#include<stdio.h>
#include<string.h>
int main()
{
	char s[20];
	int length,flag=0,i;
	printf("enter the string=");
	gets(s);
	length=strlen(s);
	for(i=0;i<length;i++)
	{
		if(s[i]!=s[length-i-1])
		{
			flag=1;
			break;
			
		}
	}
	if(flag=0)
	printf("not a palindrome");
	else
	printf("string is palindrome");
}
