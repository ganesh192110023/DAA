
#include <stdio.h>
#include <conio.h>
int main()
{
int min, max, i, j, count;
printf("Enter Your First Number\n");
scanf("%d", &min);
printf("Enter Your Last Number\n");
scanf("%d", &max);
for(i=min; i<=max; i++)
{
count=1;
for(j=2; j<=i; j++)
{
if(i % j == 0)
count++;
}
if(count==2)
printf("%d\t",i);
}
}
