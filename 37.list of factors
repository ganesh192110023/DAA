#include <stdio.h>
int main() 
{
    int n,i;

    printf("Enter a positive integer: ");
    scanf("%d", &n);
    printf("Factors of %d: ", n);
    for(i = 1; i <= n; i++) {
        if (n % i == 0) {
            printf("%d ", i);
        }
    }
    printf("\n");


    if (n <= 0) {
        printf("Invalid input. Please enter a positive integer.\n");
        return 1;
    }

    
}
